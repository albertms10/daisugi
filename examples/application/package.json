{
  "name": "@daisugi/application",
  "description": "Daisugi is a minimalist functional middleware engine.",
  "contributors": [
    {
      "name": "Alexander Sviridov",
      "email": "sviridoff.mail@gmail.com",
      "url": "https://github.com/sviridoff"
    }
  ],
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daisugiland/daisugi.git",
    "directory": "examples/application"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./src/application.js",
  "scripts": {
    "start": "node --es-module-specifier-resolution=node ./src/application.js",
    "doctor": "clinic doctor --on-port 'autocannon localhost:3001' -- node --es-module-specifier-resolution=node ./src/application.js",
    "bubble": "clinic bubbleprof --on-port 'autocannon localhost:$PORT' -- node --es-module-specifier-resolution=node ./src/application.js",
    "flame": "clinic flame --on-port 'autocannon localhost:$PORT' -- node --es-module-specifier-resolution=node ./src/application.js"
  },
  "dependencies": {
    "@daisugi/daisugi": "^0.0.1",
    "@daisugi/oumi": "^0.0.1",
    "@daisugi/oza": "^0.0.1"
  },
  "devDependencies": {
    "artillery": "^1.7.5",
    "autocannon": "^7.3.0",
    "clinic": "^9.0.0"
  }
}
