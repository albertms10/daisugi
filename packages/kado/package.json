{
  "name": "@daisugi/kado",
  "description": "Kado is a minimal and unobtrusive inversion of control container.",
  "contributors": [
    {
      "name": "Alexander Sviridov",
      "email": "sviridoff.mail@gmail.com",
      "url": "https://github.com/sviridoff"
    }
  ],
  "version": "0.1.8",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/daisugiland/daisugi.git",
    "directory": "packages/kado"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/kado.js",
      "require": "./dist/cjs/kado.js"
    }
  },
  "main": "./dist/esm/kado.js",
  "types": "dist/types/kado.d.ts",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts",
    "src"
  ],
  "keywords": [
    "daisugi",
    "kado",
    "ioc",
    "dependency-injection",
    "injection",
    "dependency"
  ],
  "scripts": {
    "prepublishOnly": "tsc --build ./tsconfig.json && tsc --project ./tsconfigCJS.json && yarn run test && minimize-js dist",
    "build": "rm -rf dist && tsc --build ./tsconfig.json && tsc --project ./tsconfigCJS.json",
    "tdd": "NODE_OPTIONS=--experimental-vm-modules jest --watchAll",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@daisugi/kintsugi": "^0.3.7"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.9",
    "jest": "^27.3.1",
    "minimize-js": "^1.2.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
}
